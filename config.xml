<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
	xmlns:gap   = "http://phonegap.com/ns/1.0"
	id          = "P7VQXPMT9U.com.zac.phonegap.*"
	version     = "0.0.1">

	<name>Frame Rate Calculater</name>
	
	<description>Calculates framerates and tells you what is the slowest speed you can go based on input framerates.</description>
	
	<author href="http://www.byu.edu" email="zac.eskelsen@byu.edu">Zac Eskelsen</author>
	
	<preference name="webviewbounce" value="false" />
	<preference name="prerendered-icon" value="true" />
	<preference name="ios-statusbarstyle" value="black-translucent" />
	<preference name="fullscreen" value="true" />
	
	<icon src = "icon.png"  />
	
	<gap:splash src="assets/images/template/startup_landscape.jpg" gap:platform="ios" width="1024" height="768" />
	<gap:splash src="assets/images/template/startup_portrait.jpg" gap:platform="ios" width="768" height="1024" />

</widget>